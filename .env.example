# Thordata Go SDK examples configuration
THORDATA_SCRAPER_TOKEN=your_scraper_token_here

# Optional (required for tasks/locations examples)
THORDATA_PUBLIC_TOKEN=your_public_token_here
THORDATA_PUBLIC_KEY=your_public_key_here

# Optional (required for tasks example)
THORDATA_SPIDER_ID=your_spider_id_here
THORDATA_SPIDER_NAME=your_spider_name_here
THORDATA_TASK_FILE_NAME=demo_task
THORDATA_TASK_PARAMETERS_JSON={"url":"https://example.com"}

# Optional: HTTP proxy for restricted networks (e.g. Clash)
HTTP_PROXY=http://127.0.0.1:7890
HTTPS_PROXY=http://127.0.0.1:7890